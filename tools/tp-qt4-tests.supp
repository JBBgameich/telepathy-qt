    fun:dbus_bus_register
    fun:internal_bus_get
    fun:_ZN15QDBusConnection12connectToBusENS_7BusTypeERK7QString
}

{
    Match rule registration messages
    Memcheck:Leak
    ...
    fun:dbus_message_new_method_call
    fun:dbus_bus_add_match
}

