libdir=@CMAKE_INSTALL_FULL_LIBDIR@
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@

Name: TelepathyQt@QT_VERSION_MAJOR@
Description: Qt utility library for the Telepathy framework
Version: @PACKAGE_VERSION@
Requires.private: Qt@QT_VERSION_MAJOR@Core >= @QT_MIN_VERSION@, Qt@QT_VERSION_PC@DBus >= @QT_MIN_VERSION@, Qt@QT_VERSION_PC@Network >= @QT_MIN_VERSION@, Qt@QT_VERSION_PC@Xml >= @QT_MIN_VERSION@
Libs: -L${libdir} -ltelepathy-qt@QT_VERSION_MAJOR@
Cflags: -I${includedir}/telepathy-qt@QT_VERSION_MAJOR@
